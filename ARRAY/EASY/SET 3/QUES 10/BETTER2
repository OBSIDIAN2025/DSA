// By using XOR method
#include <iostream>
using namespace std;
int main() {
    int n = 5;
    int arr[] = {1, 2, 3, 5};
    int xor1 = 0;
    for(int i = 1; i <= n; i++)
    {
        xor1 = xor1 ^ i;
    }
    int xor2 = 0;
    for(int i = 0; i < n - 1; i++)
    {
        xor2 = xor2 ^ arr[i];
    }
    
    cout << int(xor1 ^ xor2) << endl;
}
// Can be simplified as
#include <iostream>
using namespace std;
int main() {
    int n = 5;
    int arr[] = {1, 2, 3, 5};
    int xor1 = 0, xor2 = 0;
    for(int i = 0; i < n - 1; i++)
    {
        xor2 = xor2 ^ arr[i];
        xor1 = xor1 ^ (i + 1);
    }
    xor1 = xor1 ^ n;
    cout << int(xor1 ^ xor2) << endl;
}
